    <select id="selectBySubjectname" resultMap="SubjectCount">
        select a.subjectname,a.count from (select subjectname,count(*) as count from economyform group by subjectname) a order by count desc
    </select>

    <select id="selectByEntnameAndDate" resultMap="SubjectCount">
        select a.subjectname,a.count from
        (select subjectname,count(*) as count from economyform
        WHERE subjectname is not null AND recorddate BETWEEN #{begindate} AND #{enddate} group by subjectname) a order by count desc
    </select>

    <select id="selectcountbyfinishregion" resultType="integer">
        SELECT COUNT
	( * )
FROM
	tasklist
	A LEFT JOIN economyform b ON A.caseguid = b.caseguid
WHERE
	A.formtype = '03'
	AND nextnodeid = 'end'
	AND b.region = #{regionstr}
	AND A.updatedate BETWEEN #{begindate} and #{enddate}
    </select>

    <select id="searchEconomyformList" resultType="com.chinaweal.sdcs.crcs.business.entity.Economyform">
        select t.id,t.caseguid,t.rqsttitle,t.appealsource,t.transfernumber,t.incommingtype,
        t.detailaddress,t.subordinateplate,t.transdepartment, t.checkunit,t.othersunit,
        t.severity,t.productcategory,t.otherproductcategory,t.filed,t.incomingcommpany,
        t.rqstperson,t.rqstnumber,t.gender,t.isprofessional,t.subjectname,t.linknumber,t.updatedate,
        t.dealcompany,t.region,t.subjectaddress,t.reportprovidelbcodeid,t.objectcategorycodeid,
        t.objname,t.acceptdate,t.feedbackdate,t.assignmentdate,t.filingcasesdate,t.finalfeedbackdate,
        t.acceptancenumber,t.transferbooknumber,t.keyword,t.isweb,t.shoppingplatform,t.problemdescribe
        from economyform t inner join taskList a on t.caseguid = a.caseguid
        <where>
            <if test="map != null">
                <if test="map.rqsttitle != null and map.rqsttitle !=''">
                    and t.rqsttitle like '%' || #{map.rqsttitle} || '%'
                </if>
                <if test="map.subjectname != null and map.subjectname !=''">
                    and t.subjectname like '%' || #{map.subjectname} || '%'
                </if>
            </if>
            and systemsource = '01'
        </where>
    </select>

